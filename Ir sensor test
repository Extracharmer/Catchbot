const int ir1 = 4;
const int ir2 = 5;

void setup() {
  Serial.begin(9600);

  pinMode(ir1, INPUT_PULLUP);
  pinMode(ir2, INPUT_PULLUP);

  Serial.println("IR test started");
  Serial.println("IR1  IR2");
}

void loop() {
  Serial.print(digitalRead(ir1));
  Serial.print("    ");
  Serial.println(digitalRead(ir2));

  delay(200);
}
